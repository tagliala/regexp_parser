#!/usr/bin/env ruby

require 'bundler/setup'
require 'regexp_parser'
require 'regexp_property_values'

RL = Regexp::Lexer
RP = Regexp::ParserFzs
RS = Regexp::ScannerFzs
PV = RegexpPropertyValues

def lex(...);   Regexp::Lexer.lex(...)    end
def parse(...); Regexp::ParserFzs.parse(...) end
def scan(...);  Regexp::ScannerFzs.scan(...) end

require 'irb'
IRB.start(__FILE__)
